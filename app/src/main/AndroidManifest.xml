<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yah.androidfinal">

    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Home"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">



        <activity android:name=".LivingRoomActivity"
            android:theme="@style/AppTheme.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".HouseMainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            />
        <activity android:name=".Weather"
            android:theme="@style/AppTheme.NoActionBar"
            />
        <activity android:name=".HouseTemp"
            android:theme="@style/AppTheme.NoActionBar"
            />
        <activity android:name=".Garage"
            android:theme="@style/AppTheme.NoActionBar"
            />

    </application>

</manifest>